<div class="container">
  {{# each mobiles}}
    <div class="row">
    {{# each this}}
        <div class="col-md-4">
              <div class="thumbnail">
                <img src="{{this.imagePath}}" alt="it's an image" class="img-responsive">
                <div class="caption">
                  <h4 class="pull-right">&#8377 {{this.price}}</h4>
                  <h4><a href="#">{{this.title}}</a></h4>
                </div>
                <div>
                  <p>
                    {{this.description}}
                  </p>
                </div>
                <div class="space-ten"></div>
                <div class="btn-ground text-center">
                    <a href="/add-to-cart5/{{this._id}}"><button type="button" class="btn btn-primary"><i class="fa fa-shopping-cart"></i> Add To Cart</button></a>
                    <a href="/details/mob-detail/{{this._id}}"><button type="button" class="btn btn-primary"><i class="fa fa-shopping-cart"></i> Add To Cart</button></a>
                </div>
                <div class="space-ten"></div>
              </div>
            </div>
          {{/each}}
    </div>
    {{/each}}
</div>
